    "@astrojs/sitemap": "^3.1.4",
    "@astrojs/vue": "5.1.1",
    "@baselime/edge-logger": "0.4.2",
    "@fontsource/inter": "^5.2.8",
    "@resvg/resvg-wasm": "^2.6.2",
    "@unocss/reset": "66.5.4",
    "astro": "5.15.1",
    "astro-expressive-code": "0.41.3",
    "astro-mermaid": "^1.1.0",
    "astro-seo": "^0.8.3",
    "drizzle-orm": "0.44.7",
    "marked": "16.4.1",
