    "@vueuse/core": "^10.9.0",
    "drizzle-kit": "0.31.5",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-format": "^0.1.1",
    "lint-staged": "16.2.6",
