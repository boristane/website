{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "NODE_OPTIONS='--max-old-space-size=7168' astro build",
    "preview": "astro build && wrangler pages dev",
